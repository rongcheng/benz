body {
	background: #fff;
	margin: 0;
	padding: 0;
	font-family: verdana, Arial;
	font-size: 13px;
	color: #444;
}
a {
	font-family:verdana;
	font-size:14px;
	color:#000099;
	text-decoration: none;
}
form { margin:0; padding:0 }

table {
	font-family:verdana, Arial;
	font-size:14px
}
#res input { width:70px; font-size:12px; font-family:verdana }
#res td { text-indent:5px; font-size:16px; font-family:Sans-serif, Arial; height:20px }
#dir {
	font-family:Tahoma, Geneva, sans-serif;
	font-size:14px;
}
#err { font-size:14px; margin-left:20px; }
.c0 { color:#760303; }
.mg { vertical-align:middle; padding-bottom:8px;padding-right:10px }
.hpurl {
	font-family:Tahoma, Geneva, sans-serif;
	font-size:16px;
	color:#5258DB;
	text-decoration: none;
}



/*Menu css*/
#submenu {float:left;width:100%; text-align:center;margin:0; background: #fafafa;border-bottom:1px solid #e9e9e9;border-top:1px solid #e9e9e9}
#submenu_inner  {width:990px;margin:auto;text-align:left;overflow:visible;}
#submenu_inner_inner  {width:990px;}
#submenu_inner_inner a {
  color:#666;
	display:block;width:140px;
	overflow:hidden;
	height:35px;
	line-height:35px;
	float:left;
	text-decoration:none;
	margin:0;padding:0;
	text-align:center;
	font-size:14px;
	border-left:1px solid #e9e9e9;
	background:#fff;
}
#submenu_inner_inner #this_page {background:#2C6C83;color:#fff}
#submenu_inner_inner a:hover {color:#fff;background:#2C6C83}



/*div content css*/
.div_content {width:850px;margin:0 auto; color: #596878;}
h2 { font-size: 28px; font-weight: normal; line-height: 1.2em; margin-bottom: 10px; color: #000; }
h3 {
	font-size: 24px;
	font-weight: bold;
	line-height: 1.2em;
	margin-bottom: 10px;
	color: #000;
}
h4 {
	font-size: 18px;
	font-weight: bold;
	line-height: 1.2em;
	margin-bottom: 10px;
	color: #000;
} 

/*Page Left css*/
.sbox { border: 1px solid #ccc; padding: 20px 20px 10px 20px; margin-bottom: 20px; }
.sbox ul { margin: 0; padding: 0;}
.sbox h4 { font-size: 14px; margin: 0 0 10px;line-height: 1.2em; }
.sidebar h4 { margin: 10px 0; }

/* Page Footer css*/

#footer {
	border-top: 1px solid #dcdcdc;
	background-color:#ccc
	height: 80px;
	padding-top: 7px;
	color: #666;
}
.footer a:visited {
	color: #666666;text-decoration: none;
}
.footer a:link {
	color: #666666;text-decoration: none;
}
.footer a:hover {
	color: #FF9900;text-decoration: underline;
}

.grid_2{width:140px}
.alpha{margin-left:0}
.omega{margin-right:0}
.grid_2{
	display:inline;
	float:left;
	margin-left:10px;
	margin-right:10px;
	font-size: 14px;
	font-family: "微软雅黑";
}
